<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.burgerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Grafikekrani"></activity>
        <activity android:name=".uyelistesi" />
        <activity android:name=".odemeekrani" />
        <activity android:name=".siparisekraniliste" />
        <activity
            android:name=".yoneticiekrani"
            android:label="@string/title_activity_yoneticiekrani"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".siparisekrani" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".uyeol">
            <intent-filter>
                <action android:name="android.intent.action.UYEOL" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>